{
  "name": "serverless-simple-middleware",
  "description": "Simple middleware to translate the interface of lambda's handler to request => response",
  "version": "0.0.9",
  "main": "src/index.js",
  "author": "jaeyoung.choi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lacti/serverless-simple-middleware.git"
  },
  "keywords": [
    "serverless"
  ],
  "bugs": {
    "url": "https://github.com/lacti/serverless-simple-middleware/issues"
  },
  "homepage": "https://github.com/lacti/serverless-simple-middleware#readme",
  "optionalDependencies": {
    "aws-sdk": "^2.275.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^22.4.4",
    "jsdoc": "^3.5.5",
    "prettier": "^1.11.1",
    "serverless": "^1.27.2",
    "serverless-offline": "^3.23.0"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src"
          ]
        }
      }
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "cross-fetch": "^2.2.2",
    "directory-tree": "^2.1.0",
    "diskusage": "^0.2.4",
    "p-limit": "^2.0.0"
  }
}
